
{
    "nodes": {
      "getDestination": {
        "agent": "userInputAgent",
        "params": {
          "prompt": "Where would you like to travel?"
        }
      },
      
      "recommendation": {
        "agent": "recommendationAgent",
        "inputs": {
          "destination": "getDestination.response",
          "budget": "getBudget.response",
          "duration": "getDuration.response"
        },
        "params": {
          "model": "gpt-4",
          "recommendationType": "travel"
        }
      }
    }
  }